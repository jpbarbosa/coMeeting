<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("list", :default => "List")}", meetings_path  %></li>
      <li><%= link_to "#{t("new", :default => "New")}", new_meeting_path %></li>
      <li class="active"><%= link_to "#{t("edit", :default => "Edit")}", edit_meeting_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("edit", :default => "Edit") %>Meeting</h2>
    <div class="inner">
      <%= form_for @meeting, :url => meeting_path(@meeting.link_admin), :html => { :class => :form } do |f| -%>
        <%= render :partial => "editform", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>